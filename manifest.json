{
    "name": "Cats Instead",
    "version": "0.0.4",
    "description": "Replace every image of something you don't wanna see with cats, isn't that wonderful?",
    "manifest_version": 3,
    "author": "Dario Calore",
    "action":{
        "default_popup": "index.html",
        "default_title": "CatsInstead"
    },
    "permissions": ["storage"],
    "content_scripts": [
        {
          "matches": ["https://*/*"],
          "js": ["content_script.js"]
        }
    ],
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
      }
}